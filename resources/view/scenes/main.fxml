<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import src.resources.view.font.*?>

<AnchorPane maxHeight="700.0" maxWidth="705.0" minHeight="700.0" minWidth="705.0" prefHeight="700.0" prefWidth="705.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bubbleshooter.view.scene.controller.MainController">
   <children>
      <Label fx:id="titleLabel" textFill="RED">
         <font>
            <Font size="50.0" />
         </font></Label>
      <Button fx:id="basicModeButton" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#basicMode" text="Basic Mode" textFill="RED">
         <font>
            <Font size="30.0" />
         </font></Button>
      <Button fx:id="highscoresButton" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#highscores" text="Highscores" textFill="RED">
         <font>
            <Font size="30.0" />
         </font></Button>
      <Button fx:id="quitButton" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#quit" text="Quit" textFill="RED">
         <font>
            <Font size="30.0" />
         </font></Button>
      <Button fx:id="survivalModeButton" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#survivalMode" text="Survival Mode" textFill="RED">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
